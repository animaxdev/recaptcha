{
    "name": "vinkla/recaptcha",
    "description": "A reCAPTCHA package with optional Laravel support",
    "license": "MIT",
    "keywords": ["recaptcha", "google", "laravel"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        }
    ],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "guzzlehttp/guzzle": "^5.3.1 || ^6.2.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "illuminate/contracts": "5.3.*",
        "illuminate/support": "5.3.*",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.4"
    },
    "suggest": {
        "laravel/framework": "Enables Laravel support."
    },
    "autoload": {
        "files": [
            "./src/helpers.php"
        ],
        "psr-4": {
            "Vinkla\\Recaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Recaptcha\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
